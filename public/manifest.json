{
  "manifest_version": 3,
  "name": "Pokemon Showdown Extension",
  "version": "0.0.1",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://blank.org/", "https://play.pokemonshowdown.com/*"],
      "run_at": "document_idle",
      "js": [],
      "css": [],
      "media": []
    }
  ],
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"]
    }
  ]
}
